{
    "version": 2,
    "name": "dad",
    "builds": [
        {"src": "pkg/now/*/*.go", "use": "@now/go"},
        {"src": "bin/*.json", "use": "@now/static"}
    ],
    "routes": [
        {"src": "/hn/(.*)", "dest": "/pkg/now/hn/hn.go"},
        {"src": "/joke/(.*)", "dest": "/pkg/now/joke/$1"}
    ]
}